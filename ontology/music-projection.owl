    <!-- https://w3id.org/polifonia/ontology/music-projection/EnharmonicInterval -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-projection/EnharmonicInterval">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Interval"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-projection/spansOver"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/music-projection/DiatonicStep"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
