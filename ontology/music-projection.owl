        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Chord"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Chord"/>
