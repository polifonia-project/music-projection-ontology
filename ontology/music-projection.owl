        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/isStartingPitchClassOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Interval"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/PitchClass"/>
