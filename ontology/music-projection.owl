    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/hasSpecificIntervalValue -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasSpecificIntervalValue">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/isSpecificIntervalValueOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Interval"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/SpecificInterval"/>
