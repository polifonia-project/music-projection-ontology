    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/isCadenceTypeOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/isCadenceTypeOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/isClassifiedBy"/>
