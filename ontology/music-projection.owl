                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/music-projection/spansOver"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="https://w3id.org/polifonia/ontology/music-projection/DiatonicStep"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An enharmonic interval (EIV) is the difference between two EPCs

E.g. 5 semitones</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">Hentschel, J., Moss, F. C., McLeod, A., Neuwirth, M., &amp; Rohrmeier, M. (2022). Towards a unified model of chords in western harmony.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Enharmonic interval</rdfs:label>
        <skos:altLabel xml:lang="en">EIV</skos:altLabel>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/EnharmonicPitchClass -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-projection/EnharmonicPitchClass">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/GenericPitchClass"/>
        <owl:disjointWith rdf:resource="https://w3id.org/polifonia/ontology/music-projection/SpelledPitchClass"/>
        <rdfs:comment xml:lang="en">An enharmonic pitch class (EPC) is a numeric value from 1 to 12. It represents an absoute value for a pitch class. It is not possible to identify modfiers, becuase of the existance of enharmonic pitches.

E.g. 9</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">Hentschel, J., Moss, F. C., McLeod, A., Neuwirth, M., &amp; Rohrmeier, M. (2022). Towards a unified model of chords in western harmony.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Enharmonic pitch class</rdfs:label>
        <skos:altLabel xml:lang="en">EPC</skos:altLabel>
    </owl:Class>
    


