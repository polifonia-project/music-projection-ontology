    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/isComposedOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/isComposedOf">
