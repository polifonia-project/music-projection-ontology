    <!-- https://w3id.org/polifonia/ontology/music-projection/hasInversion -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasInversion">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">Relates a chord to its inversion.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://en.wikipedia.org/wiki/Inversion_(music)</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has inversion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/hasOctave -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasOctave">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/Pitch"/>
