        <skos:altLabel xml:lang="en">GIV</skos:altLabel>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/GenericPitchClass -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/music-projection/GenericPitchClass">
