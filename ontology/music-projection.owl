        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/PitchClass"/>
        <rdfs:comment xml:lang="en">Relation between a key and its tonic PC.</rdfs:comment>
        <rdfs:label xml:lang="en">has tonic</rdfs:label>
    </owl:ObjectProperty>
    


