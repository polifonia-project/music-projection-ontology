        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/PitchClassRole"/>
        <rdfs:comment xml:lang="en">Role of a PC element in a PC selection.</rdfs:comment>
        <rdfs:label xml:lang="en">has pitch class role</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/hasQuality -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasQuality">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/isQualityOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/SpecificInterval"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/IntervalQuality"/>
        <rdfs:comment xml:lang="en">Relation between an interval and its quality.</rdfs:comment>
        <rdfs:label xml:lang="en">has quality</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/hasStartingPitchClass -->

