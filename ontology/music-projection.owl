        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/isCadenceTypeOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Cadence"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/CadenceType"/>
        <rdfs:comment xml:lang="en">The relationship between a cadence and a type it is classified as.</rdfs:comment>
        <rdfs:label xml:lang="en">has cadence type</rdfs:label>
    </owl:ObjectProperty>
    


