        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/IntervalQuality"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/SpecificInterval"/>
        <rdfs:comment xml:lang="en">Relation between a quality and an interval.</rdfs:comment>
        <rdfs:label xml:lang="en">is quality of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/isSpecificIntervalValueOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/isSpecificIntervalValueOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/SpecificInterval"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Interval"/>
        <rdfs:comment xml:lang="en">The amount of pitch classes ranged by an interval.</rdfs:comment>
        <rdfs:label xml:lang="en">is specific interval value of</rdfs:label>
    </owl:ObjectProperty>
    


