        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Pitch"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/PitchClass"/>
        <rdfs:comment xml:lang="en">The relation between a pitch and the pitch class represented.</rdfs:comment>
        <rdfs:label xml:lang="en">has pitch class</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/hasQuality -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasQuality">
