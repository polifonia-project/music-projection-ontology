    <!-- https://w3id.org/polifonia/ontology/music-projection/DiatonicStep -->

    <rdfs:Datatype rdf:about="https://w3id.org/polifonia/ontology/music-projection/DiatonicStep">
        <rdfs:comment xml:lang="en">A diatonic step is an absolute step with respect to the reference note, i.e. a fifth.</rdfs:comment>
