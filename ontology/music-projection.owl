        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/Pitch"/>
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/PitchClass"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Defines the relationship between a chord and the pitch class or pitch it is composed of.</rdfs:comment>
        <rdfs:label xml:lang="en">is composed of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/isDurationOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/isDurationOf">
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Duration"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Note"/>
        <rdfs:comment xml:lang="en">Relation between a duration and a note it is used in.</rdfs:comment>
        <rdfs:label xml:lang="en">is duration of</rdfs:label>
    </owl:ObjectProperty>
    


