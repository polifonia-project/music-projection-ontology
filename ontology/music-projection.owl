        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/isAmbitusTypeOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Ambitus"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/AmbitusType"/>
        <rdfs:comment xml:lang="en">The relation between an ambitus and the type it is classified as.</rdfs:comment>
        <rdfs:label xml:lang="en">has ambitus type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/hasCadenceType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasCadenceType">
