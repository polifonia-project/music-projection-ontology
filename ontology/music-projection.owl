    <!-- https://w3id.org/polifonia/ontology/music-projection/hasAccidental -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasAccidental">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/isAccidentalOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/Note"/>
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/SpecificScaleDegree"/>
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/SpelledPitchClass"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Accidental"/>
        <rdfs:comment xml:lang="en">Relation between a pitch class and an accidental.</rdfs:comment>
        <rdfs:label xml:lang="en">has accidental</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/hasAmbitusType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasAmbitusType">
