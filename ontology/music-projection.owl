        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Pitch"/>
        <rdfs:comment xml:lang="en">Relation between a pitch class and the pitches it is instiatied as.</rdfs:comment>
        <rdfs:label xml:lang="en">is pitch class of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/isPitchOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/isPitchOf">
