    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/isEnharmonicallyEquivalentTo">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-projection/isEnharmonicallyEquivalentTo"/>
