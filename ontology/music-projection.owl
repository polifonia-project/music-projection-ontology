    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/Interval">
        <rdfs:label xml:lang="en">Interval</rdfs:label>
        <rdfs:comment xml:lang="en">An interval is a difference between two pitch classes.</rdfs:comment>
        <rdfs:seeAlso xml:lang="en">https://en.wikipedia.org/wiki/Interval_(music)</rdfs:seeAlso>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/NaturalAccidental">
        <rdfs:label xml:lang="en">Natural accidental</rdfs:label>
        <rdfs:comment xml:lang="en">A natural accidental is an accidental that cancels previous accidentals.</rdfs:comment>
        <rdfs:seeAlso xml:lang="en">https://en.wikipedia.org/wiki/Natural_(music)</rdfs:seeAlso>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/Pitch">
        <rdfs:comment xml:lang="en">A pitch is a perceptual property of a sound, related to a frequency. It is defined as the combination of a pitch class and an octave.</rdfs:comment>
        <rdfs:seeAlso xml:lang="en">https://en.wikipedia.org/wiki/Pitch_(music)</rdfs:seeAlso>
        <rdfs:label xml:lang="en">Pitch</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-projection/PitchClass">
        <rdfs:label xml:lang="en">Pitch class</rdfs:label>
        <skos:altLabel xml:lang="en">PC</skos:altLabel>
        <rdfs:seeAlso xml:lang="en">https://en.wikipedia.org/wiki/Pitch_class</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">The set of alla pitches that are a whole number of octaves apart.</rdfs:comment>
    </rdf:Description>
    


