        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-projection/Duration"/>
        <rdfs:comment xml:lang="en">Relation between a note and its duration.</rdfs:comment>
        <rdfs:label xml:lang="en">has duration</rdfs:label>
    </owl:ObjectProperty>
    


