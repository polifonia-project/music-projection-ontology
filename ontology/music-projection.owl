        <rdfs:label xml:lang="en">has specific interval value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-projection/hasStartingPitchClass -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-projection/hasStartingPitchClass">
